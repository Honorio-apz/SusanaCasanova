---
import Container from "@components/container.astro";
import { Icon } from "astro-icon/components";

const { margin } = Astro.props;

const footermenu = {
  company: [
    {
      title: "About",
      path: "/about",
    },
    {
      title: "Careers",
      path: "#",
    },
    {
      title: "Blog",
      path: "/blog",
    },
    {
      title: "Contact",
      path: "/contact",
    },
  ],
  product: [
    {
      title: "For Startups",
      path: "#",
    },
    {
      title: "Features",
      path: "#",
    },
    {
      title: "Pricing",
      path: "/pricing",
    },
    {
      title: "Integrations",
      path: "#",
    },
  ],
  resources: [
    {
      title: "Help Center",
      path: "#",
    },
    {
      title: "Guides",
      path: "#",
    },
    {
      title: "Documentation",
      path: "#",
    },
    {
      title: "Uptime Status",
      path: "#",
    },
  ],
};
